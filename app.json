{
  "expo": {
    "name": "Currency Converter",
    "slug": "CurrencyConverter",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rnovec98.CurrencyConverter"
    },
    "androidStatusBar": {
      "translucent": false
    },
    "android": {
      "package": "com.rnovec98.CurrencyConverter"
    }
  }
}
